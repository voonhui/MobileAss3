                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textViewR3" />

            <View
                android:id="@+id/divider16"
                android:layout_width="380dp"
                android:layout_height="3dp"
                android:layout_marginTop="16dp"
